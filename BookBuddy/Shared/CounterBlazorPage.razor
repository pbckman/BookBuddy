<div class="card mb-5">
    <h3>CounterBlazorPage</h3>

    <p>Current count: @count </p>
    <button class="btn btn-success" @onclick="IncrementCount">Increment</button>
</div>

@code {
    public int count = 0;

    private void IncrementCount()
    {
        count++;
        StateHasChanged();
    }
}